<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="AppTheme.Base">
        <item name="android:colorPrimary">@color/primaryColor</item>
        <item name="android:colorPrimaryDark">@color/primaryColorDark</item>
        <item name="android:colorAccent">@color/accentColor</item>
    </style>

    <style name="emergencyTheme" parent="ThemeOverlay.AppCompat.Dark">
        <item name="android:background">@color/warnColor</item>
    </style>

    <style name="AppTheme.busStopTheme">
        <item name="android:background">@color/busColor</item>
    </style>

    <style name="AppTheme.notificationsTheme">
        <item name="android:background">@color/notificationsColor</item>
    </style>
</resources>